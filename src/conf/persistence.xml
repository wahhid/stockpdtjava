<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="StockPDTPU" transaction-type="JTA">
    <jta-data-source>jdbc/opnampdt</jta-data-source>
    <class>org.jakc.stockpdt.entity.Employee</class>
    <class>org.jakc.stockpdt.entity.Gondola</class>
    <class>org.jakc.stockpdt.entity.Periode</class>
    <class>org.jakc.stockpdt.entity.Site</class>
    <class>org.jakc.stockpdt.entity.Transstock</class>
    <class>org.jakc.stockpdt.entity.Transtockdetail</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <validation-mode>NONE</validation-mode>
    <properties/>
  </persistence-unit>
</persistence>
